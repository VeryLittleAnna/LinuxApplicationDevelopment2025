#: src/main.c:32
#, c-format
msgid "Использование: mastermind [ОПЦИЯ]\n"
msgstr "Usage: mastermind [OPTION]\n"

#: src/main.c:33
#, c-format
msgid "Опции:\n"
msgstr "Options:\n"

#: src/main.c:34
#, c-format
msgid "  -h, --help     Показать эту справку\n"
msgstr "  -h, --help     Show this help message\n"

#: src/main.c:68
#, c-format
msgid "Ошибка: неизвестная опция '%s'\n"
msgstr "Error: unknown option '%s'\n"

#: src/main.c:69
#, c-format
msgid "Посмотрите '%s --help' для справки.\n"
msgstr "Try '%s --help' for help.\n"

#: src/main.c:77
#, c-format
msgid "Загадана строка (%d цифр из %s)\n"
msgstr "A string has been generated (%d digits from %s)\n"

#: src/main.c:78
#, c-format
msgid "Команды для игры: подсказка, выход, история\n"
msgstr "Game commands: hint, exit, history\n"

#: src/main.c:79
#, c-format
msgid ""
"Ответ на догадку в формате (n, m), где n - количество верных символов с "
"учетом позиции, m - количество верных символов на неверных позициях\n"
msgstr ""
"Response to a guess in the format (n, m), where n is the number of correct symbols in the correct positions, m is the number of correct symbols in the wrong positions\n"

#: src/main.c:81
#, c-format
msgid "Введите вашу догадку (%d цифр из %s): "
msgstr "Enter your guess (%d digits from %s): "

#: src/main.c:90
msgid "подсказка"
msgstr "hint"

#: src/main.c:94
msgid "история"
msgstr "history"

#: src/main.c:98
msgid "выход"
msgstr "exit"

#: src/main.c:102
#, c-format
msgid "Догадка должна содержать ровно %d символов!\n"
msgstr "The guess must contain exactly %d characters!\n"

#: src/main.c:106
#, c-format
msgid "Догадка должна содержать только символы из алфавита: %s\n"
msgstr "The guess must contain only characters from the alphabet: %s\n"

#: src/lib/mastermind.c:139
#, c-format
msgid "(%d, %d)\n"
msgstr "(%d, %d)\n"

#: src/lib/mastermind.c:169
#, c-format
msgid "История в формате: догадка - (быки, коровы)\n"
msgstr "History in format: guess - (bulls, cows)\n"

#: src/lib/mastermind.c:239
#, c-format
msgid "Игра прервана. Правильный ответ был: %s\n"
msgstr "Game aborted. The correct answer was: %s\n"

#: src/lib/mastermind.c:245
msgid "Поздравляю! Вы победили!\n"
msgstr "Congratulations! You won!\n"

#: src/lib/mastermind.c:247
#, c-format
msgid "Вы победили с %d подсказками!\n"
msgstr "You won with %d hints!\n"

#: src/lib/mastermind.c:251
#, c-format
msgid "Игра окончена. Правильный ответ был: %s\n"
msgstr "Game over. The correct answer was: %s\n"

#: src/lib/mastermind.c:270
#, c-format
msgid "Верная цифра на позиции %d - %c\n"
msgstr "The correct digit at position %d is %c\n"
